{
  "name": "Cloudinary Videos",
  "description": "Cloudinary Videos",
  "group": "Cloudinary",
  "attribute_definition_groups": [
    {
      "id": "Video",
      "name": "Cloudinary Video Configuration",
      "attribute_definitions": [
        {
          "id": "asset_sel",
          "type": "custom",
          "required": true,
          "editor_definition": {
            "type": "cloudinary.video_selector_trigger",
            "configuration": {
              "mlType": "video"
            }
          }
        },
        {
          "id": "poster_sel",
          "type": "custom",
          "required": false,
          "editor_definition": {
            "type": "cloudinary.radio_group",
            "configuration": {
              "label": "Video Poster",
              "options": {"Auto": "emit", "First Frame": "emit", "Custom": "openMl"},
              "groupName": "poster",
              "defaultVal": "Auto",
              "mlType": "image"
            }
          }
        },

        {
          "id": "controls_showcontrols",
          "name": "Show Controls",
          "type": "boolean",
          "default_value": true,
          "required": false
        },
        {
          "id": "controls_autoplay",
          "name": "Auto Play",
          "type": "boolean",
          "default_value": false,
          "required": false
        },
        {
          "id": "controls_loopvideo",
          "name": "Loop Video",
          "type": "boolean",
          "default_value": false,
          "required": false
        },
        {
          "id": "controls_theme",
          "name": "Select Theme",
          "type": "enum",
          "values": [
            "Dark",
            "Light"
          ],
          "default_value": "Dark",
          "required": false
        },
        {
          "id": "controls_muted",
          "name": "Mute Video",
          "type": "boolean",
          "default_value": false,
          "required": false
        },
        {
          "id": "controls_no_volume",
          "name": "Do Not Show Volume Controls",
          "type": "boolean",
          "default_value": false,
          "required": false
        },
        {
          "id": "controls_jump",
          "name": "Show +/- 10sec control",
          "type": "boolean",
          "default_value": false,
          "required": false
        },
        {
          "id": "controls_no_fullscreen",
          "name": "Do Not Allow Fullscreen",
          "type": "boolean",
          "default_value": false,
          "required": false
        },
        {
          "id": "overlay",
          "name": "Image Overlay",
          "type": "custom",
          "required": false,
          "editor_definition": {
            "type": "cloudinary.overlay_selector"
          }
        },
        {
          "id": "textOverlay",
          "name": "Text Overlay",
          "type": "custom",
          "required": false,
          "editor_definition": {
            "type": "cloudinary.text_overlay_selector"
          }
        }
        ]
      }
    ],
    "region_definitions": []
  }